array1 = input("Enter the elements of the first array separated by spaces: ").split()
array2 = input("Enter the elements of the second array separated by spaces: ").split()
array1 = [int(element) for element in array1]
array2 = [int(element) for element in array2]
if array1 == array2:
    print("The two arrays are the same.")
else:
    print("The two arrays are not the same.")
